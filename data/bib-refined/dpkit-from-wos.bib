    location = {{Amsterdam}},
    pages = {109474},
    publisher = {{Elsevier}},
    shortjournal = {Comput. Mater. Sci.},
    title = {Catalytic Materials and Chemistry Development Using a Synergistic Combination of Machine Learning and Ab Initio Methods},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/4},
