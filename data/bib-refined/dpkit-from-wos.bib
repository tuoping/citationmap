    journaltitle = {Journal of Computational Physics},
    langid = {english},
    location = {{San Diego}},
    pages = {110085},
    publisher = {{Academic Press Inc Elsevier Science}},
    shortjournal = {J. Comput. Phys.},
    shorttitle = {{{PFNN}}},
    title = {{{PFNN}}: {{A}} Penalty-Free Neural Network Method for Solving a Class of Second-Order Boundary-Value Problems on Complex Geometries},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/2},
