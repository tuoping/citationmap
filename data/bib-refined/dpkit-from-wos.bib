    publisher = {{Iop Publishing Ltd}},
    shortjournal = {Chin. Phys. Lett.},
    title = {A {{Linear Frequency Principle Model}} to {{Understand}} the {{Absence}} of {{Overfitting}} in {{Neural Networks}}},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/2},
    urldate = {2021-08-06},
