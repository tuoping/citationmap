    shortjournal = {J. Phys.-Condes. Matter},
    shorttitle = {Structure and Dynamics of Warm Dense Aluminum},
    title = {Structure and Dynamics of Warm Dense Aluminum: A Molecular Dynamics Study with Density Functional Theory and Deep Potential},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/4},
    urldate = {2021-08-06},
