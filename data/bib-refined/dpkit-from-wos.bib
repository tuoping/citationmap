    publisher = {{Wiley-V C H Verlag Gmbh}},
    shortjournal = {Phys. Status Solidi-Rapid Res. Lett.},
    title = {Structure and {{Dynamics}} of {{Supercooled Liquid Ge2Sb2Te5}} from {{Machine}}-{{Learning}}-{{Driven Simulations}}},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/3},
    urldate = {2021-08-06},
