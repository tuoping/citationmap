    journaltitle = {Journal of Alloys and Compounds},
    langid = {english},
    location = {{Lausanne}},
    pages = {675--682},
    publisher = {{Elsevier Science Sa}},
    shortjournal = {J. Alloy. Compd.},
    title = {Bergman-Type Medium Range Order in Amorphous {{Zr77Rh23}} Alloy Studied by Ab Initio Molecular Dynamics Simulations},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/5},
    urldate = {2021-08-06},
