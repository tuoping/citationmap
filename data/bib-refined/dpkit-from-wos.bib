    langid = {english},
    location = {{Berlin}},
    number = {1},
    pages = {19},
    publisher = {{Nature Research}},
    shortjournal = {npj Comput. Mater.},
    title = {Predicting Adsorption Ability of Adsorbents at Arbitrary Sites for Pollutants Using Deep Transfer Learning},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/2},
    urldate = {2021-08-06},
