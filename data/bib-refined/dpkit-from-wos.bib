    publisher = {{Amer Chemical Soc}},
    shortjournal = {J. Phys. Chem. Lett.},
    title = {Inclusion of {{Machine Learning Kernel Ridge Regression Potential Energy Surfaces}} in {{On}}-the-{{Fly Nonadiabatic Molecular Dynamics Simulation}}},
    url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/6},
    urldate = {2021-08-06},
