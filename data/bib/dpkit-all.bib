@article{linUnravellingFastAlkaliIon2021a,
  title = {Unravelling the {{Fast Alkali}}-{{Ion Dynamics}} in {{Paramagnetic Battery Materials Combined}} with {{NMR}} and {{Deep}}-{{Potential Molecular Dynamics Simulation}}},
