  file = {C\:\\Users\\abby\\Zotero\\storage\\IEVJTPP6\\Chen 等。 - 2020 - Competitive Effect of Disorder and Defects on Dyna.pdf;C\:\\Users\\abby\\Zotero\\storage\\PZL8RB9F\\2006.html}
}

