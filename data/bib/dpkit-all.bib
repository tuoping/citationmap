@article{sinzWaveletScatteringNetworks2020a,
  title = {Wavelet Scattering Networks for Atomistic Systems with Extrapolation of Material Properties},
  author = {Sinz, Paul and Swift, Michael W. and Brumwell, Xavier and Liu, Jialin and Kim, Kwang Jin and Qi, Yue and Hirn, Matthew},
