@article{youngTransferableActivelearningStrategy,
  title = {A Transferable Active-Learning Strategy for Reactive Molecular Force Fields},
  author = {Young, Tom A. and Johnston-Wood, Tristan and Deringer, Volker L. and Duarte, Fernanda},
  journaltitle = {Chemical Science},
  shortjournal = {Chem. Sci.},
