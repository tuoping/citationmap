  file = {C\:\\Users\\abby\\Zotero\\storage\\JCXV58UF\\Xie 等。 - 2021 - Bayesian force fields from active learning for sim.pdf;C\:\\Users\\abby\\Zotero\\storage\\LL2RIYEP\\s41524-021-00510-y.html}
