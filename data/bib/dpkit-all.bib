@article{schererKernelBasedMachineLearning2020,
  title = {Kernel-{{Based Machine Learning}} for {{Efficient Simulations}} of {{Molecular Liquids}}},
  author = {Scherer, Christoph and Scheid, Rene and Andrienko, Denis and Bereau, Tristan},
  date = {2020-05-12},
  journaltitle = {Journal of Chemical Theory and Computation},
  shortjournal = {J. Chem. Theory Comput.},
