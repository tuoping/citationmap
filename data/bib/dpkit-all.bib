@article{liMultilevelFineTuningClosing2021a,
  title = {Multilevel {{Fine}}-{{Tuning}}: {{Closing Generalization Gaps}} in {{Approximation}} of {{Solution Maps Under}} a {{Limited Budget}} for {{Training Data}}},
  shorttitle = {Multilevel {{Fine}}-{{Tuning}}},
  author = {Li, Zhihan and Fan, Yuwei and Ying, Lexing},
  date = {2021},
  journaltitle = {Multiscale Modeling \& Simulation},
  shortjournal = {Multiscale Model. Simul.},
  volume = {19},
  number = {1},
  pages = {344--373},
  publisher = {{Siam Publications}},
  location = {{Philadelphia}},
  issn = {1540-3459},
  doi = {10.1137/20M1326404},
  url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/2},
  urldate = {2021-08-06},
  abstract = {In scientific machine learning, regression networks have been recently applied to approximate solution maps (e.g., the potential-ground state map of the Schrodinger equation). In this paper, we aim to reduce the generalization error without spending more time on generating training samples. However, to reduce the generalization error, the regression network needs to be fit on a large number of training samples (e.g., a collection of potential-ground state pairs). The training samples can be produced by running numerical solvers, which takes significant time in many applications. In this paper, we aim to reduce the generalization error without spending more time on generating training samples. Inspired by few-shot learning techniques, we develop the multilevel fine-tuning algorithm by introducing levels of training: we first train the regression network on samples generated at the coarsest grid and then successively fine-tune the network on samples generated at finer grids. Within the same amount of time, numerical solvers generate more samples on coarse grids than on fine grids. We demonstrate a significant reduction of generalization error in numerical experiments on challenging problems with oscillations, discontinuities, or rough coefficients. Further analysis can be conducted in the neural tangent kernel regime, and we provide practical estimators to the generalization error. The number of training samples at different levels can be optimized for the smallest estimated generalization error under the constraint of budget for training data. The optimized distribution of budget over levels provides practical guidance with theoretical insight as in the celebrated multilevel Monte Carlo algorithm.},
  langid = {english},
  keywords = {few-shot learning,generalization,multilevel method,neural tangent kernel,parametric model,rademacher},
  annotation = {WOS:000636051200013},
  file = {C\:\\Users\\abby\\Zotero\\storage\\22T57NLL\\Li 等。 - 2021 - Multilevel Fine-Tuning Closing Generalization Gap.pdf;C\:\\Users\\abby\\Zotero\\storage\\YR64TG6V\\Li 等。 - 2021 - Multilevel Fine-Tuning Closing Generalization Gap.pdf;C\:\\Users\\abby\\Zotero\\storage\\TM9AWGVX\\20M1326404.html}
