}

@article{Lu2018,
  title = {86 {{PFLOPS Deep Potential Molecular Dynamics}} Simulation of 100 Million Atoms with Ab Initio Accuracy},
  author = {Lu, D and Wang, H and Chen, M and Lin, L and Car, R and E, Weinan and 2021, undefined},
