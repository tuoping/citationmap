@article{wangElectronicallyDriven1D2021a,
  title = {Electronically {{Driven 1D Cooperative Diffusion}} in a {{Simple Cubic Crystal}}},
  author = {Wang, Yong and Wang, Junjie and Hermann, Andreas and Liu, Cong and Gao, Hao and Tosatti, Erio and Wang, Hui-Tian and Xing, Dingyu and Sun, Jian},
