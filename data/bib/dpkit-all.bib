  shorttitle = {A {{DFT}} Accurate Machine Learning Description of Molten {{ZnCl2}} and Its Mixtures},
  author = {Pan, Gechuanqi and Chen, Pin and Yan, Hui and Lu, Yutong},
  date = {2020-12},
