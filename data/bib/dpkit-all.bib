  keywords = {accuracy,atoms li,basis-sets,chemistry,energy surfaces,gaussian approximation potentials,Gaussian process regression,Kernel ridge regression,Machine learning,molecular-dynamics,Neural   networks,Quantum chemistry,zeta valence quality},
