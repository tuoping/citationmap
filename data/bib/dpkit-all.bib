@article{herrCompressingPhysicsAutoencoder2019a,
  title = {Compressing Physics with an Autoencoder: {{Creating}} an Atomic Species Representation to Improve Machine Learning Models in the Chemical Sciences},
  shorttitle = {Compressing Physics with an Autoencoder},
  author = {Herr, John E. and Koh, Kevin and Yao, Kun and Parkhill, John},
  date = {2019-08-28},
  journaltitle = {Journal of Chemical Physics},
