@article{sugisawaGaussianProcessModel2020a,
  title = {Gaussian Process Model of 51-Dimensional Potential Energy Surface for Protonated Imidazole Dimer},
  author = {Sugisawa, Hiroki and Ida, Tomonori and Krems, R.},
  date = {2020-09-21},
  journaltitle = {Journal of Chemical Physics},
