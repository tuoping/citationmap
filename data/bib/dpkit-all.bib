  keywords = {artificial bee colony,basin hopping,basin-hopping method,bee colony algorithm,boron clusters,chemical clusters,differential evolution algorithm,double-hexagonal vacancy,embedded-genetic-algorithm,gas-phase,genetic   algorithm,geometry   optimization,global optimization,lennard-jones   clusters,potential energy surface,potential-energy surface},
