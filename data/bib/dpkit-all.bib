@article{shenQuantumTrajectoryMeanField2019a,
  title = {Quantum {{Trajectory Mean}}-{{Field Method}} for {{Nonadiabatic Dynamics}} in {{Photochemistry}}},
  author = {Shen, Lin and Tang, Diandong and Xie, Binbin and Fang, Wei-Hai},
  date = {2019-08-29},
  journaltitle = {Journal of Physical Chemistry A},
  shortjournal = {J. Phys. Chem. A},
