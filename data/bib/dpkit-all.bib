@article{wangMachineLearningCoarseGrained2019,
  title = {Machine {{Learning}} of {{Coarse}}-{{Grained Molecular Dynamics Force Fields}}},
  author = {Wang, Jiang and Olsson, Simon and Wehmeyer, Christoph and Perez, Adria and Charron, Nicholas E. and de Fabritiis, Gianni and Noe, Frank and Clementi, Cecilia},
  options = {useprefix=true},
  date = {2019-05-22},
  journaltitle = {Acs Central Science},
  shortjournal = {ACS Central Sci.},
  volume = {5},
