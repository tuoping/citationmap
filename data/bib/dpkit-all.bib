@article{hanUniformlyAccurateMachine2019,
  title = {Uniformly Accurate Machine Learning-Based Hydrodynamic Models for Kinetic Equations},
  author = {Han, Jiequn and Ma, Chao and Ma, Zheng and E, Weinan},
