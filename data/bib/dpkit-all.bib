@article{leoniNonclassicalNucleationPathways2021a,
  title = {Nonclassical {{Nucleation Pathways}} in {{Stacking}}-{{Disordered Crystals}}},
  author = {Leoni, Fabio and Russo, John},
  date = {2021-07-09},
  journaltitle = {Physical Review X},
