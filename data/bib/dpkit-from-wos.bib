  file = {C\:\\Users\\abby\\Zotero\\storage\\M6RWSLFQ\\Deringer - 2020 - Modelling and understanding battery materials with.pdf;C\:\\Users\\abby\\Zotero\\storage\\MGKN4PUA\\hkvalidate.perfdrive.com.html}
}

