@inproceedings{oehmckeModelingH2ORutileTiO22020,
  title = {Modeling {{H2O}}/{{Rutile}}-{{TiO2}}(110) {{Potential Energy Surfaces}} with {{Deep Networks}}},
  booktitle = {2020 {{International Joint Conference}} on {{Neural Networks}} (Ijcnn)},
  author = {Oehmcke, Stefan and Teusch, Thomas and Petersen, Thorben and Kluener, Thorsten and Kramer, Oliver},
  date = {2020},
