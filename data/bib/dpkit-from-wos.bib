@article{paleicoBinHashMethod2021,
  title = {A Bin and Hash Method for Analyzing Reference Data and Descriptors in Machine Learning Potentials},
  author = {Paleico, Martin Leandro and Behler, Joerg},
  date = {2021-09},
  journaltitle = {Machine Learning-Science and Technology},
