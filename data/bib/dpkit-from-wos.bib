@article{peigneyFourierbasedMachineLearning2021a,
  title = {A {{Fourier}}-Based Machine Learning Technique with Application in Engineering},
  author = {Peigney, Michael},
  date = {2021-02-15},
  journaltitle = {International Journal for Numerical Methods in Engineering},
  shortjournal = {Int. J. Numer. Methods Eng.},
  volume = {122},
  number = {3},
  pages = {866--897},
  publisher = {{Wiley}},
  location = {{Hoboken}},
