@article{chenGoldSegregationImproves2021,
  title = {Gold {{Segregation Improves Electrocatalytic Activity}} of {{Icosahedron Au}}@{{Pt}} Nanocluster: Insights from Machine Learning},
  shorttitle = {Gold {{Segregation Improves Electrocatalytic Activity}} of {{Icosahedron Au}}@{{Pt}} Nanocluster},
  author = {Chen, Dingming and Lai, Zhuangzhuang and Zhang, Jiawei and Chen, Jianfu and Hu, Peijun and Wang, Haifeng},
  date = {2021-07-02},
  journaltitle = {Chinese Journal of Chemistry},
