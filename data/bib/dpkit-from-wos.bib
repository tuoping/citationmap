  pages = {144102},
  issn = {0021-9606, 1089-7690},
  doi = {10/gmgftv},
  url = {http://arxiv.org/abs/1812.06572},
  urldate = {2021-08-11},
  abstract = {We propose a new molecular simulation framework that combines the transferability, robustness and chemical flexibility of an ab initio method with the accuracy and efficiency of a machine learning model. The key to achieve this mix is to use a standard density functional theory (DFT) simulation as a preprocessor for the atomic and molecular information, obtaining a good quality electronic density. General, symmetry preserving, atom-centered electronic descriptors are then built from this density to train a neural network to correct the baseline DFT energies and forces. These electronic descriptors encode much more information than local atomic environments, allowing a simple neural network to reach the accuracy required for the problem of study at a negligible additional cost. The balance between accuracy and efficiency is determined by the baseline simulation. This is shown in results where high level quantum chemical accuracy is obtained for simulations of liquid water at standard DFT cost, or where high level DFT-accuracy is achieved in simulations with a low-level baseline DFT calculation, at a significantly reduced cost.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Physics - Chemical Physics,Physics - Computational Physics},
  file = {C\:\\Users\\abby\\Zotero\\storage\\IAUHPY4T\\Dick 和 Fernandez-Serra - 2019 - Learning from the Density to Correct Total Energy .pdf}
}

@article{dralHierarchicalMachineLearning2020a,
  title = {Hierarchical Machine Learning of Potential Energy Surfaces},
  author = {Dral, Pavlo O. and Owens, Alec and Dral, Alexey and Csanyi, Gabor},
  date = {2020-05-29},
  journaltitle = {Journal of Chemical Physics},
  shortjournal = {J. Chem. Phys.},
  volume = {152},
  number = {20},
  publisher = {{Amer Inst Physics}},
  location = {{Melville}},
  issn = {0021-9606},
  doi = {10.1063/5.0006498},
  url = {https://www.webofscience.com/wos/alldb/summary/44a3404b-9775-4cfb-8e8f-3f1fab40830f-035e7f8d/date-descending/4},
  urldate = {2021-08-06},
  abstract = {We present hierarchical machine learning (hML) of highly accurate potential energy surfaces (PESs). Our scheme is based on adding predictions of multiple Delta -machine learning models trained on energies and energy corrections calculated with a hierarchy of quantum chemical methods. Our (semi-)automatic procedure determines the optimal training set size and composition of each constituent machine learning model, simultaneously minimizing the computational effort necessary to achieve the required accuracy of the hML PES. Machine learning models are built using kernel ridge regression, and training points are selected with structure-based sampling. As an illustrative example, hML is applied to a high-level ab initio CH3Cl PES and is shown to significantly reduce the computational cost of generating the PES by a factor of 100 while retaining similar levels of accuracy (errors of similar to 1 cm(-1)).},
  langid = {english},
  keywords = {atoms,basis-sets,chemistry,combined 1st-principles   calculation,heat,molecular-dynamics simulations,neural-network correction,quantum mechanics/molecular mechanics},
  annotation = {WOS:000537898900006},
  file = {C\:\\Users\\abby\\Zotero\\storage\\M9VBW5QK\\Dral 等。 - 2020 - Hierarchical machine learning of potential energy .pdf;C\:\\Users\\abby\\Zotero\\storage\\PD9Z3JFY\\Dral 等。 - 2020 - Hierarchical machine learning of potential energy .pdf;C\:\\Users\\abby\\Zotero\\storage\\ZXAXTP7W\\5.html}
