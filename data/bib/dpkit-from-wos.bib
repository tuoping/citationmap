}

@article{haMachineLearningAssistedExcited2021,
  title = {Machine {{Learning}}-{{Assisted Excited State Molecular Dynamics}} with the {{State}}-{{Interaction State}}-{{Averaged Spin}}-{{Restricted Ensemble}}-{{Referenced Kohn}}-{{Sham Approach}}},
  author = {Ha, Jong-Kwon and Kim, Kicheol and Min, Seung Kyu},
