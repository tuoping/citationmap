@article{kimReactionPathForceMatching2021,
  title = {Reaction {{Path}}-{{Force Matching}} in {{Collective Variables}}: {{Determining Ab Initio QM}}/{{MM Free Energy Profiles}} by {{Fitting Mean Force}}},
  shorttitle = {Reaction {{Path}}-{{Force Matching}} in {{Collective Variables}}},
  author = {Kim, Bryant and Snyder, Ryan and Nagaraju, Mulpuri and Zhou, Yan and Ojeda-May, Pedro and Keeton, Seth and Hege, Mellisa and Shao, Yihan and Pu, Jingzhi},
  date = {2021-08-10},
