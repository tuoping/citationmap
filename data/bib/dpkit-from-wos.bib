  title = {Retention and Recycling of Deuterium in Liquid Lithium-Tin Slab Studied by First-Principles Molecular Dynamics},
  author = {Zheng, Daye and Shen, Zhen-Xiong and Chen, Mohan and Ren, Xinguo and He, Lixin},
  date = {2021-01},
