  keywords = {carbide,ceramics,complexion,Deep learning potential,Grain boundary segregation,hf,Mechanical properties,mechanical-properties,mo,Molecular dynamics,nb,solute atoms,ta,zirconium,ZrB2-based UHTCs},
  annotation = {WOS:000564257400003},
