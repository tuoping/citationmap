@article{zhouAtomicstatedependentScreeningModel2021a,
  title = {Atomic-State-Dependent Screening Model for Hot and Warm Dense Plasmas},
  author = {Zhou, Fuyang and Qu, Yizhi and Gao, Junwen and Ma, Yulong and Wu, Yong and Wang, Jianguo},
  date = {2021-06-30},
  journaltitle = {Communications Physics},
  shortjournal = {Commun. Phys.},
  volume = {4},
